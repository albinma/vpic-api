{
  "name": "vpic-api",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "test": "NODE_ENV=test TZ=utc LOG_LEVEL=warn vitest --run --passWithNoTests --logHeapUsage test/*",
    "gen:types": "openapi-typescript data/openapi/v1.yaml -o src/api/types/openapi.d.ts --empty-objects-unknown && prettier --write src/api/types/openapi.d.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "db:gen:types": "pnpm kysely-codegen"
  },
  "dependencies": {
    "@scalar/express-api-reference": "0.6.8",
    "@sinclair/typebox": "0.34.33",
    "@tediousjs/connection-string": "0.6.0",
    "@types/compression": "1.7.5",
    "compression": "1.8.0",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-openapi-validator": "5.4.9",
    "helmet": "8.1.0",
    "kysely": "0.28.0",
    "lodash": "4.17.21",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "tarn": "3.0.2",
    "tedious": "18.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@types/express": "5.0.1",
    "@types/supertest": "6.0.3",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "kysely-codegen": "0.18.3",
    "lint-staged": "15.5.1",
    "openapi-typescript": "7.6.1",
    "pino-pretty": "13.0.0",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vite": "6.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
